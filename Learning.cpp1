#include <iostream>
using namespace std;

int main(){
    string name; // variable to store users name
    cout << "Hello, Welcome to my Program." << endl; // output welcome
    cout << "What is your name?" << endl; // ask for name
    cin >> name; // get input into name variable

    cout << "Nice to meet you " << name << endl; // output greeting with name

    return 0;
}
